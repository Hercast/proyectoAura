#include <Wire.h> 
#include <LiquidCrystal_I2C.h>

//Crear el objeto lcd  direcci√≥n  0x3F y 16 columnas x 2 filas
LiquidCrystal_I2C lcd(0x27,16,2);  //

void setup() {
  // Inicializar el LCD
  lcd.init();
  
  //Encender la luz de fondo.
  lcd.backlight();
  
}

void loop() {
lcd.print("Hola Mundo");
  delay(100);
  lcd.clear();
}
